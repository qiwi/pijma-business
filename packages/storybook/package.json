{
  "name": "@pijma/business-storybook",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "clean": "yarn infra:clean",
    "start": "concurrently -k -r 'npm:start:tester' 'npm:start:server'",
    "start:server": "start-storybook --quiet --port 8080",
    "start:tester": "wait-on http-get://localhost:8080 && test-storybook --watchAll --index-json --url http://localhost:8080",
    "build": "build-storybook --quiet --output-dir lib",
    "test": "concurrently -k -r -s first 'npm:test:tester' 'npm:test:server'",
    "test:server": "http-server lib --port 8080 --silent",
    "test:tester": "wait-on http-get://localhost:8080 && test-storybook --index-json --url http://localhost:8080",
    "style": "yarn infra:style"
  },
  "dependencies": {
    "@pijma/business-button": "workspace:*",
    "@pijma/business-heading": "workspace:*",
    "@pijma/business-icon": "workspace:*",
    "@pijma/business-icons": "workspace:*",
    "@pijma/business-paragraph": "workspace:*",
    "@pijma/business-text-field": "workspace:*",
    "@pijma/business-wrapper": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "7.18.6",
    "@babel/preset-env": "7.18.6",
    "@babel/preset-react": "7.18.6",
    "@babel/preset-typescript": "7.18.6",
    "@pijma/business-infra": "workspace:*",
    "@storybook/addon-essentials": "6.4.22",
    "@storybook/addon-interactions": "6.4.22",
    "@storybook/addon-jest": "6.4.22",
    "@storybook/addon-react-native-web": "0.0.18",
    "@storybook/builder-webpack5": "6.4.22",
    "@storybook/jest": "0.0.10",
    "@storybook/manager-webpack5": "6.4.22",
    "@storybook/react": "6.4.22",
    "@storybook/test-runner": "0.4.0",
    "@storybook/testing-library": "0.0.13",
    "@testing-library/jest-dom": "5.16.4",
    "babel-loader": "8.2.5",
    "babel-plugin-react-native-web": "0.18.6",
    "babel-plugin-tsconfig-paths-module-resolver": "1.0.3",
    "concurrently": "7.2.2",
    "http-server": "14.1.1",
    "jest": "27.5.1",
    "react-native": "0.69.1",
    "react-native-reanimated": "2.9.1",
    "react-native-svg": "12.3.0",
    "react-native-web": "0.18.6",
    "react-test-renderer": "18.2.0",
    "storybook-addon-swc": "1.1.7",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "wait-on": "6.0.1",
    "webpack": "5.73.0"
  }
}
