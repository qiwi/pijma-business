    "access": "public"
  },
  "files": [
    "lib",
    "README.md"
  ],
  "scripts": {
    "clean": "yarn infra:clean",
    "build": "yarn infra:build",
    "build:mjs": "yarn infra:build:mjs",
    "build:cjs": "yarn infra:build:cjs",
    "build:tsc": "yarn infra:build:tsc",
    "style": "yarn infra:style"
  },
  "dependencies": {
    "@pijma/business-icon": "workspace:*"
  },
