{
  "name": "@pijma/business-button",
  "description": "Pijma Business Button",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "./lib/cjs/index.cjs",
  "module": "./lib/mjs/index.mjs",
  "types": "./lib/dts/index.d.ts",
  "react-native": "./lib/jsx/index.js",
  "exports": {
    ".": {
      "types": "./lib/dts/index.d.ts",
      "import": "./lib/mjs/index.mjs",
      "require": "./lib/cjs/index.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiwi/pijma-business.git"
  },
  "bugs": {
    "url": "https://github.com/qiwi/pijma-business/issues"
  },
  "homepage": "https://qiwi.github.io/pijma-business/",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib",
    "README.md"
  ],
  "scripts": {
    "clean": "rm -rf lib",
    "build": "concurrently 'npm:build:*'",
    "build:mjs": "tsc --project tsconfig.mjs.json && tsc-esm-fix --tsconfig tsconfig.mjs.json --ext .mjs",
    "build:cjs": "tsc --project tsconfig.cjs.json && tsc-esm-fix --target lib/cjs --ext .cjs",
    "build:jsx": "tsc --project tsconfig.jsx.json",
    "build:dts": "tsc --project tsconfig.dts.json",
    "style": "eslint --fix src && prettier --loglevel silent --write src"
  },
  "dependencies": {
    "@pijma/business-icon": "workspace:*"
  },
  "peerDependencies": {
    "@pijma/react-native": ">=1.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0",
    "react-native": ">=0.50.0",
    "tslib": ">=2.0.0"
  },
  "devDependencies": {
    "@pijma/react-native": "1.4.0",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/react-native": "0.69.2",
    "concurrently": "7.2.2",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-qiwi": "1.17.3",
    "prettier": "2.7.1",
    "prettier-config-qiwi": "1.7.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.69.1",
    "tsc-esm-fix": "2.18.0",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "packageManager": "yarn@4.0.0-rc.9"
}
